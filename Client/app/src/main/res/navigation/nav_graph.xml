<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.dsckiet.hackathonhelpapp.screen.HomeFragment"
        android:label="HomeFragment" />
    <fragment
        android:id="@+id/profileFragment"
        android:name="com.dsckiet.hackathonhelpapp.screen.ProfileFragment"
        android:label="ProfileFragment" />
    <fragment
        android:id="@+id/volunteerFragment"
        android:name="com.dsckiet.hackathonhelpapp.screen.VolunteerFragment"
        android:label="VolunteerFragment" >
        <action
            android:id="@+id/action_volunteerFragment_to_getVolunteerFragment2"
            app:destination="@id/getVolunteerFragment2" />
        <action
            android:id="@+id/action_volunteerFragment_to_helpVaultFragment"
            app:destination="@id/helpVaultFragment" />
    </fragment>
    <fragment
        android:id="@+id/getVolunteerFragment2"
        android:name="com.dsckiet.hackathonhelpapp.screen.GetVolunteerFragment"
        android:label="GetVolunteerFragment" />
    <fragment
        android:id="@+id/helpVaultFragment"
        android:name="com.dsckiet.hackathonhelpapp.screen.HelpVaultFragment"
        android:label="HelpVaultFragment" />
